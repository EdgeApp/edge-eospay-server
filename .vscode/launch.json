  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "EOS: Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/eos-name-server.ts",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "preLaunchTask": "tsc",
      "args": ["eos"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "TLOS: Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/eos-name-server.ts",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "preLaunchTask": "tsc",
      "args": ["tlos"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Test Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/test.js",
      // "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      // "preLaunchTask": "tsc"
    }
  ]
}